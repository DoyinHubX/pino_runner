{
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "script"
  },
  "rules": {
    "no-console": "warn",
    "consistent-return": "error"
  }
}



